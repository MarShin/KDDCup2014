totalCount = essays.shape[0]
for i in range(essays.shape[1]):
    nullcount = essays[essays[essays.columns[i]].isnull()].shape[0]
    percentage = float(nullcount)/float(totalCount) *100
    if(percentage>0):
        print(essays.columns[i],percentage,'%')
        
